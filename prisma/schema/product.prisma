model Product {
  id                  Int          @id @default(autoincrement())
  title               String       @unique
  url_source          String
  description         String
  brandId             Int
  categoryId          Int
  sku                 String
  price               Float
  features            String       
  specifications      String
  images              String
  warranty            String
  delivery_information String
  createdAt           DateTime     @default(now())
  updatedAt           DateTime     @updatedAt

  @@map("product")
}
